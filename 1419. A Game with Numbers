#include<iostream>
#include<cstdio>
#include<cstring>
#include<cmath>
#include<algorithm>
#include<vector>

bool cmp( int a, int b ) 
{
   return a > b;
}

#define MOD 1000000007
#define PI 3.141592654
#define TRUE 1
#define FALSE 0
#define LL long long int

using namespace std;

int main()
{
        int n;
        while(scanf("%d",&n)!=EOF)
        {
                if(n%10==0)
                        printf("2\n");
                else
                        printf("1\n%d\n",n%10);
        }
        return 0;
}
