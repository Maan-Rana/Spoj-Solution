#include<stdio.h>
#include<string.h>
int main()
{
    int t,i,l;
    char s[100];
    scanf("%d",&t);
    while(t--)
    {
              scanf("%s",s);
              l=strlen(s);
              for(i=0;i<l;i++)
              {
                              if((s[i]=='a')||(s[i]=='e')||(s[i]=='i')||(s[i]=='o')||(s[i]=='u'))
                                   printf("%c",s[i]-32);
                              else
                                  printf("%c",s[i]);
              }
              printf("\n");
    }
    return 0;
}
